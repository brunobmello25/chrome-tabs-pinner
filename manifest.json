{
  "manifest_version": 3,
  "name": "AutoPin Google Calendar",
  "version": "1.0",
  "description": "Automatically opens and pins Chrome tabs for given urls.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open and pin Chrome tabs"
  }
}
